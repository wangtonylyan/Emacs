{
    "[haskell]": {
        "editor.tabSize": 2
    },
    "bracket-pair-colorizer-2.activeScopeCSS": [
        "borderStyle : none none solid none",
        "borderWidth : 2px",
        "borderColor : #ff8046"
    ],
    "bracket-pair-colorizer-2.colorMode": "Independent",
    "bracket-pair-colorizer-2.colors": [],
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "bracket-pair-colorizer-2.scopeLineCSS": [
        "borderStyle : solid",
        "borderWidth : 1px",
        "borderColor : #ff8046"
    ],
    "bracket-pair-colorizer-2.showBracketsInGutter": true,
    "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
    "cmake.configureOnOpen": false,
    "code-runner.executorMap": {
        "haskell": "stack runghc",
        "python": "python3 -u"
    },
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "debug.console.fontSize": 13,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.autoClosingBrackets": "always",
    "editor.bracketPairColorization.enabled": false,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorWidth": 4,
    "editor.definitionLinkOpensInPeek": true,
    "editor.dragAndDrop": false,
    "editor.foldingHighlight": false,
    "editor.fontFamily": "'Fira Mono', 'Source Han Sans SC', Consolas, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnPaste": true,
    "editor.matchBrackets": "near",
    "editor.minimap.maxColumn": 40,
    "editor.minimap.size": "fill",
    "editor.mouseWheelZoom": true,
    "editor.parameterHints.cycle": true,
    "editor.renderWhitespace": "selection",
    "editor.suggest.localityBonus": true,
    "editor.suggestSelection": "first",
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?·～！￥…（）—【】、；：‘’“”，。《》？ ",
    "editor.wordWrapColumn": 120,
    "errorLens.delay": 5000,
    "errorLens.fontSize": "12",
    "errorLens.margin": "20ch",
    "errorLens.messageTemplate": "[$severity] $message",
    "explorer.enableDragAndDrop": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": true,
    "files.autoGuessEncoding": true,
    "files.eol": "\n",
    "files.exclude": {
        "**/*.hi": true,
        "**/*.o": true,
        "**/.vscode": true,
        "**/__pycache__": true
    },
    "files.trimTrailingWhitespace": true,
    "filesize.showGzip": false,
    "git.ignoreMissingGitWarning": true,
    "go.buildOnSave": "workspace",
    "go.toolsManagement.autoUpdate": true,
    "go.useLanguageServer": true,
    "hediet.vscode-drawio.local-storage": "",
    "hediet.vscode-drawio.theme": "Kennedy",
    "hungryDelete.enableSmartBackspace": false,
    "hungryDelete.followAbovelineIndent": true,
    "hungryDelete.keepOneSpace": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.configuration.runtimes": [
        {
            "name": "Java-11",
            "default": true,
            "path": "/usr/lib/jvm/java-11-openjdk-amd64/bin/java"
        }
    ],
    "java.refactor.renameFromFileExplorer": "preview",
    "javascript.format.semicolons": "insert",
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindowMode": "perFile",
    "macros": {},
    "markdown.preview.fontFamily": "'Fira Mono', 'Source Han Sans SC', Consolas, monospace",
    "markdown.preview.fontSize": 18,
    "markdown.preview.lineHeight": 1.5,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.lineNumbers": "on",
    "notebook.navigation.allowNavigateToSurroundingCells": false,
    "notebook.outline.showCodeCells": true,
    "partialDiff.enableTelemetry": false,
    "python.languageServer": "Pylance",
    "python.showStartPage": false,
    "redhat.telemetry.enabled": false,
    "remote.SSH.remotePlatform": {
        "Ubuntu18.04Server": "linux",
        "Ubuntu_Server_18.04": "linux"
    },
    "scm.diffDecorations": "minimap",
    "search.smartCase": true,
    "sortJSON.orderOverride": [
        "name",
        "version",
        "description",
        "label",
        "type",
        "command",
        "executable",
        "args"
    ],
    "sortJSON.orderUnderride": [
        "dependencies",
        "devDependencies"
    ],
    "subtleBrackets.pairs": [
        {
            "close": ")",
            "open": "("
        },
        {
            "close": "]",
            "open": "["
        },
        {
            "close": "}",
            "open": "{"
        },
        {
            "close": ">",
            "open": "<"
        }
    ],
    "subtleBrackets.style": {
        "borderColor": "#ff8046",
        "borderStyle": "none none solid none",
        "borderWidth": "2px"
    },
    "sync.forceUpload": true,
    "sync.gist": "80a2e25aaa161904704503897ec992ed",
    "sync.removeExtensions": false,
    "tabnine.disable_file_regex": [
        "[.]js$"
    ],
    "tabnine.experimentalAutoImports": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.rendererType": "canvas",
    "terminal.integrated.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?·～！￥…（）—【】、；：‘’“”，。《》？ ",
    "todo-tree.highlights.enabled": true,
    "todo-tree.tree.showScanModeButton": false,
    "typescript.format.semicolons": "insert",
    "update.mode": "none",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.title": "${dirty}${activeEditorLong}${separator}${appName}",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": -0.5,
    "workbench.colorCustomizations": {
        "[Solarized Custom Light]": {
            "editor.foreground": "#000000",
            "editor.selectionBackground": "#ff8046",
            "errorLens.errorBackground": "#fff0",
            "errorLens.errorBackgroundLight": "#fff0",
            "errorLens.hintBackground": "#fff0",
            "errorLens.hintBackgroundLight": "#fff0",
            "errorLens.infoBackground": "#fff0",
            "errorLens.infoBackgroundLight": "#fff0",
            "errorLens.warningBackground": "#fff0",
            "errorLens.warningBackgroundLight": "#fff0",
            "sideBar.foreground": "#000000",
            "tab.activeForeground": "#000000"
        }
    },
    "workbench.colorTheme": "Solarized Custom Light",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.showTabs": true,
    "workbench.editor.tabCloseButton": "off",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "yaml.format.enable": true,
    "gitlens.keymap": "none",
    "gitlens.hovers.enabled": false,
    "gitlens.hovers.avatars": false,
    "gitlens.hovers.pullRequests.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.views.commits.avatars": false,
    "gitlens.views.tags.avatars": false,
    "gitlens.views.fileHistory.avatars": false,
    "gitlens.views.lineHistory.avatars": false,
    "gitlens.views.branches.avatars": false,
    "gitlens.views.remotes.avatars": false,
    "gitlens.views.compare.avatars": false,
    "gitlens.views.search.avatars": false,
    "gitlens.views.contributors.avatars": false,
    "gitlens.views.repositories.avatars": true,
    "gitlens.blame.avatars": true
}

